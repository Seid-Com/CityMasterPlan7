{
    "name": "cityplan/master-plan-gis",
    "type": "project",
    "description": "City Master Plan Web GIS Application with CodeIgniter 4.5 and PostGIS",
    "homepage": "https://github.com/cityplan/master-plan-gis",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.5",
        "ext-pgsql": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "gasparesganga/php-shapefile": "^3.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^10.5"
    },
    "suggest": {
        "ext-fileinfo": "Improves mime type detection for file uploads"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
